---

import { dynamoDb } from "@music-sheet-tracker/core/dynamoDb";
import { Table } from "sst/node/table";

const response = await dynamoDb.query({
	KeyConditionExpression: "musicSheetUniqueName = :musicSheetUniqueName",
	ExpressionAttributeValues: {
		":musicSheetUniqueName": "K301",
	},
	TableName: Table.SheetMusic.tableName,
});
const jsonResponse = JSON.stringify(response.Items);
---

<html lang="en">
	<head>
		<meta charset="utf-8" />
		<link rel="icon" type="image/svg+xml" href="/favicon.svg" />
		<meta name="viewport" content="width=device-width" />
		<meta name="generator" content={Astro.generator} />
		<title>Astro</title>
	</head>
	<body>
		<h1>Astro</h1>
		<div>{jsonResponse}</div>
	</body>
</html>
